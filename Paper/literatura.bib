@Article{Landau1986,
author={Landau, G. M.
and Vishkin, U.
and Nussinov, R.},
title={An efficient string matching algorithm with k differences for nucleotide and amino acid sequences.},
journal={Nucleic Acids Res},
year={1986},
month={Jan},
day={10},
volume={14},
number={1},
pages={31-46},
abstract={There are a few algorithms designed to solve the problem of the optimal alignment of one sequence, the pattern, of length m, with another, longer sequence the text, of length n. These algorithms allow mismatches, deletions and insertions. Algorithms to date run in O(mn) time. Let us define an integer, k, which is the maximal number of differences allowed. We present a simple algorithm showing that sequences can be optimally aligned in O(k2n) time. For long sequences the gain factor over the currently used algorithms is very large.},
note={3753770[pmid]},
issn={0305-1048},
url={http://www.ncbi.nlm.nih.gov/pmc/articles/PMC339353/}
}

